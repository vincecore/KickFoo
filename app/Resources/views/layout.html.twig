                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="{{ path('dashboard_index') }}">Home</a></li>
                        <li><a href="{{ path('mobile_index') }}">Start Game</a></li>
                        <li><a href="{{ path('wallofshame') }}">Wall of Shame</a></li>
                        <li><a href="{{ path('stats_topscorers') }}">Stats</a></li>
                        <li><a href="{{ path('game_live') }}">Live</a></li>
